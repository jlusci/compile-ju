function fib(n) {
    var a = 0;
    var b = 1;
    var temp_a = a;
    while (a < n ){
        print a;
        temp_a = a;
        a = b;
        b = temp_a + b;
    }
}

function main() {
	print "*********************************************************";
    print "Here are the first 20 terms in the Fibonacci sequence";
    fib(20);
    print "*********************************************************";
}